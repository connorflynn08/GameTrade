<body>
<h1 id="curh1">My Listings</h1>
<%= link_to 'Post New Item', new_sell_game_path, id:"pni" %>
<br>
<hr class="new1">
    <% @sell_games.each do |sell_game| %>
    <div class="card">
      <td><%= image_tag sell_game.picture, class: "card-img-top" %> </td>

      <div class="card-body">
        <h5 class="card-title"><td><%= sell_game.title %></td></h5>
        <p class="card-text"><td><%= sell_game.description %></td></p>
        <td><%= link_to 'Show', sell_game, class:"btn btn-primary" %></td>
        <td><% if can? :update, sell_game %><%= link_to 'Edit', edit_sell_game_path(sell_game), class:"btn btn-primary" %> <% end %></td>
        <td><% if can? :destroy, sell_game %><%= link_to 'Destroy', sell_game, method: :delete, data: { confirm: 'Are you sure?' }, class:"btn btn-primary" %><% end %></td>


  </div>
  </div>
    <% end %>
    <br>
</div>
<br>
<hr class="new2">
<br>
<br>
